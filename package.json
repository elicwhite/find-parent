    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "mocha": "^3.0.2",
    "watchify": "^3.7.0",
    "wealthfront-javascript": "^4.0.0"
  },
  "repository": {
    "type": "git",
